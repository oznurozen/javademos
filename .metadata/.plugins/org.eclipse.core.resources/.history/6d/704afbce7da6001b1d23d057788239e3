package sesliharfler;

public class Main {

	public static void main(String[] args) {
		char harf='E';
		
		switch(harf){
		case 'A':
		case 'E':
		case 'I':
		case 'Ý':
		case 'O':
		case 'Ö':
		case 'U':
		case 'Ü':
			System.out.println(harf +" Sesli harftir");
			break;
			default:
				System.out.println(harf +" Sessiz harftir");
		}

	}

}
