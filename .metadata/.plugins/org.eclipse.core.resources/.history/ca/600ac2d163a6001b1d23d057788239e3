package loopDemo;

public class Main {

	public static void main(String[] args) {
		//For
		for(int i=2;i<10;i+=2) {
			System.out.println(i);
		}
		System.out.println("For Döngü Bitti");

		int i=1;
		//While
		while(i<10) {
			System.out.println(i);
		}
		System.out.println("While Döngü Bitti");
	}

}
